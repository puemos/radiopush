<h1>Listing Channels</h1>
<div>
  <%=for channel <- @channels do %>
    <a href="/channel/<%= channel.id %>">
      <div>
        Name <%= channel.name %>
      </div>
      <div>
        ID <%= channel.id %>
      </div>
    </a>
  <% end %>
</div>
<form action="#" phx-submit="channel">
  <%= text_input :channel, :name, placeholder: "Channel name" %>
  <%= submit "Create", phx_disable_with: "Creating..." %>
</form>
